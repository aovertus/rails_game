<h1>Login</h1>
<%= form_tag session_path do %>
	<div class="form">
		<%= label_tag :email %>
		<%= text_field_tag :email %><br/>
		
		<%= label_tag :password %>
		<%= password_field_tag :password %>
		
	</div>
	<div class="actions">
		<%= submit_tag "Login", :class => "btn btn-default"%>
	</div>
<% end %>
